{
  "extends": "airbnb",
  "env": {
    "es6": true,
    "jest": true
  },
  "rules": {
    "linebreak-style": "off",
    "quotes": [
      1,
      "single"
    ],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js"
        ]
      }
    ],
    "react/destructuring-assignment": "off",
    "react/require-default-props": "off",
    "react/sort-prop-types": "warn",
    "import/prefer-default-export": "off",
    "react/no-array-index-key": "off"
  }
}